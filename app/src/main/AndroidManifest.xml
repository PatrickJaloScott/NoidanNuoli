<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.noidannuoli">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_nn_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_nn_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.NoidanNuoli">
        <activity
            android:name=".ConfirmNewActivity"
            android:exported="false"
            android:parentActivityName=".ModelActivity"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name=".ModelActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CalendarActivity"
            android:exported="false" />
        <activity
            android:name=".HistoryDetailsActivity"
            android:exported="false"
            android:parentActivityName=".HistoryActivity"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity
            android:name=".HistoryActivity"
            android:exported="false"
            android:parentActivityName=".ModelActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:parentActivityName=".ModelActivity" />
        <activity
            android:name=".TestPinActivity"
            android:exported="true"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>